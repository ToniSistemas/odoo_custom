<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="report_producttemplatelabel4x11_custom">
        <t t-call="web.basic_layout">
            <div class="page">
                <t t-set="columns" t-value="4"/>
                <t t-set="rows" t-value="11"/>
                <t t-set="price_included" t-value="True"/>
                <t t-set="page_numbers" t-value="page_numbers or 1"/>
                <div>products: <t t-esc="products"/></div>
                <t t-foreach="products" t-as="product">
                    <t t-call="product.report_productlabel"/>
                </t>
            </div>
        </t>
    </template>
</odoo>

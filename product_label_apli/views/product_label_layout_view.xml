<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_label_layout_form_inherit" model="ir.ui.view">
        <field name="name">product.label.layout.form.inherit</field>
        <field name="model">product.label.layout</field>
        <field name="inherit_id" ref="product.view_product_label_layout"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='print_format']" position="inside">
                <field name="print_format" position="attributes">
                    <attribute name="invisible">0</attribute>
                </field>
            </xpath>
        </field>
    </record>
</odoo>
